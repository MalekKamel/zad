<selector xmlns:android="http://schemas.android.com/apk/res/android">


        <item android:state_pressed="true">
            <shape>
                <gradient
                    android:startColor="#58cdf4"
                    android:centerColor="#58cdf4"
                    android:endColor="#58cdf4"
                    android:angle="270" />
                <padding android:left="5.0dip" android:top="0.0dip" android:right="0.0dip" android:bottom="5.0dip" />
                <corners android:radius="10dp"   />
            </shape>
        </item>



        <item android:state_focused="true">
            <shape>
                <gradient
                    android:startColor="#66c7f1"
                    android:centerColor="#66c7f1"
                    android:endColor="#66c7f1"
                    android:angle="270" />
                <padding android:left="5.0dip" android:top="0.0dip" android:right="0.0dip" android:bottom="5.0dip" />
                <corners android:radius="10dp"   />
            </shape>
        </item>



        <item>
            <shape>
                <gradient
                    android:startColor="#357f9f"
                    android:centerColor="#357f9f"
                    android:endColor="#357f9f"
                    android:angle="270" />
                <padding android:left="5.0dip" android:top="0dip" android:right="0dip" android:bottom="5.0dip" />

                <corners android:radius="10dp"   />
            </shape>
        </item>


    </selector>
